CONFIG_IMX6UL_IMAGE_SIGN_CERT="pki/imx6ul_hab_testkeys/IMG1_1_sha256_4096_65537_v3_usr_crt.pem"
# end of Output image
# end of imx6ul configuration

CONFIG_TOOLCHAIN_PREFIX="/opt/gcc-arm-8.3-2019.03-x86_64-arm-eabi/bin/arm-eabi-"
